{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.js"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "SESSION_SECRET": "@session_secret"
  },
  "functions": {
    "api/server.js": {
      "maxDuration": 30
    }
  }
}
